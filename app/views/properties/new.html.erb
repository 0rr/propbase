<br />
<h2>properties#new</h2>

<%= form_with scope: :property, url: properties_path, local: true do |form| %>
  <p>
    <%= form.label :premise %>
    <%= form.text_field :premise %>
  </p>
    <p>
    <%= form.label :address %>
    <%= form.text_field :address %>
  </p>
  <p>
    <%= form.label :city %>
    <%= form.text_field :city %>
  </p>
  <p>
    <%= form.label :state %>
    <%= form.text_field :state %>
  </p>
  <p>
    <%= form.label :zip %>
    <%= form.text_field :zip %>
  </p>
    <p>
    <%= form.submit %>
  </p>
<% end %>